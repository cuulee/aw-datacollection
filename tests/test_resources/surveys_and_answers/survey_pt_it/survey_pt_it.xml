      <instance>
        <survey_pt_it id="survey_pt_it">
          <user_name>Anonymous</user_name>
          <ex_note/>
          <a_integer/>
          <a_decimal/>
          <calculate/>
          <calculate_test_output/>
          <required_text/>
          <skip_example/>
          <skipable_question/>
          <repeat_test jr:template="">
            <repeating_question/>
          </repeat_test>
          <group_test>
            <select_multiple/>
            <group_within_note>
              <group_within_note/>
              <select_multiple_within/>
            </group_within_note>
          </group_test>
          <meta>
            <instanceID/>
          </meta>
        </survey_pt_it>
      </instance>
      <bind nodeset="/survey_pt_it/user_name" type="string"/>
      <bind nodeset="/survey_pt_it/ex_note" readonly="true()" type="string"/>
      <bind nodeset="/survey_pt_it/a_integer" type="int"/>
      <bind constraint=". &lt;  /survey_pt_it/a_integer " nodeset="/survey_pt_it/a_decimal" type="decimal"/>
      <bind calculate=" /survey_pt_it/a_integer + /survey_pt_it/a_decimal " nodeset="/survey_pt_it/calculate" type="string"/>
      <bind nodeset="/survey_pt_it/calculate_test_output" readonly="true()" type="string"/>
      <bind nodeset="/survey_pt_it/required_text" required="true()" type="string"/>
      <bind nodeset="/survey_pt_it/skip_example" required="true()" type="select1"/>
      <bind nodeset="/survey_pt_it/skipable_question" relevant="selected( /survey_pt_it/skip_example , 'no')" required="true()" type="string"/>
      <bind nodeset="/survey_pt_it/repeat_test/repeating_question" type="string"/>
      <bind nodeset="/survey_pt_it/group_test/select_multiple" type="select"/>
      <bind nodeset="/survey_pt_it/group_test/group_within_note/group_within_note" readonly="true()" type="string"/>
      <bind nodeset="/survey_pt_it/group_test/group_within_note/select_multiple_within" type="select"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/survey_pt_it/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/survey_pt_it/user_name">
      <label ref="jr:itext('/survey_pt_it/user_name:label')"/>
      <hint ref="jr:itext('/survey_pt_it/user_name:hint')"/>
    </input>
    <input ref="/survey_pt_it/ex_note">
      <label ref="jr:itext('/survey_pt_it/ex_note:label')"/>
    </input>
    <input ref="/survey_pt_it/a_integer">
      <label ref="jr:itext('/survey_pt_it/a_integer:label')"/>
    </input>
    <input ref="/survey_pt_it/a_decimal">
      <label ref="jr:itext('/survey_pt_it/a_decimal:label')"/>
      <hint ref="jr:itext('/survey_pt_it/a_decimal:hint')"/>
    </input>
    <input ref="/survey_pt_it/calculate_test_output">
      <label ref="jr:itext('/survey_pt_it/calculate_test_output:label')"/>
    </input>
    <input ref="/survey_pt_it/required_text">
      <label ref="jr:itext('/survey_pt_it/required_text:label')"/>
      <hint ref="jr:itext('/survey_pt_it/required_text:hint')"/>
    </input>
    <select1 ref="/survey_pt_it/skip_example">
      <label ref="jr:itext('/survey_pt_it/skip_example:label')"/>
      <item>
        <label ref="jr:itext('/survey_pt_it/skip_example/yes:label')"/>
        <value>yes</value>
      </item>
      <item>
        <label ref="jr:itext('/survey_pt_it/skip_example/no:label')"/>
        <value>no</value>
      </item>
    </select1>
    <input ref="/survey_pt_it/skipable_question">
      <label ref="jr:itext('/survey_pt_it/skipable_question:label')"/>
    </input>
    <group ref="/survey_pt_it/repeat_test">
      <label ref="jr:itext('/survey_pt_it/repeat_test:label')"/>
      <repeat nodeset="/survey_pt_it/repeat_test">
        <input ref="/survey_pt_it/repeat_test/repeating_question">
          <label ref="jr:itext('/survey_pt_it/repeat_test/repeating_question:label')"/>
        </input>
      </repeat>
    </group>
    <group ref="/survey_pt_it/group_test">
      <label ref="jr:itext('/survey_pt_it/group_test:label')"/>
      <select ref="/survey_pt_it/group_test/select_multiple">
        <label ref="jr:itext('/survey_pt_it/group_test/select_multiple:label')"/>
        <item>
          <label ref="jr:itext('/survey_pt_it/group_test/select_multiple/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/survey_pt_it/group_test/select_multiple/no:label')"/>
          <value>no</value>
        </item>
      </select>
      <group ref="/survey_pt_it/group_test/group_within_note">
        <label ref="jr:itext('/survey_pt_it/group_test/group_within_note:label')"/>
        <input ref="/survey_pt_it/group_test/group_within_note/group_within_note">
          <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/group_within_note:label')"/>
        </input>
        <select ref="/survey_pt_it/group_test/group_within_note/select_multiple_within">
          <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/select_multiple_within:label')"/>
          <item>
            <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/select_multiple_within/banana:label')"/>
            <value>banana</value>
          </item>
          <item>
            <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/select_multiple_within/pear:label')"/>
            <value>pear</value>
          </item>
          <item>
            <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/select_multiple_within/kiwi:label')"/>
            <value>kiwi</value>
          </item>
          <item>
            <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/select_multiple_within/apple:label')"/>
            <value>apple</value>
          </item>
          <item>
            <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/select_multiple_within/passion_fruit:label')"/>
            <value>passion_fruit</value>
          </item>
          <item>
            <label ref="jr:itext('/survey_pt_it/group_test/group_within_note/select_multiple_within/tomato:label')"/>
            <value>tomato</value>
          </item>
        </select>
      </group>
    </group>
  </h:body>
</h:html>
